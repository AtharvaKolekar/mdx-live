// prisma/schema.prisma
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Room {
  id        String   @id @default(cuid())
  name      String   @unique
  title     String?  
  content   String?  
  updatedAt DateTime @updatedAt
}
